{
  "author": "Joe Chung",
  "dependencies": {
    "@tanstack/svelte-query": "^5.87.1",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.13.7",
    "flowbite-svelte-icons": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.37.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.4",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/node": "^22.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.1",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "mock-match-media": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.38.7",
    "svelte-check": "^4.3.1",
    "tailwind-variants": "^3.1.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^3.2.4"
  },
  "name": "diagnostics-sv",
  "overrides": {
    "@sveltejs/kit": {
      "cookie": "^0.7.0"
    }
  },
  "private": true,
  "scripts": {
    "build": "vite build",
    "check": "npm-run-all check:sync check:type",
    "check:sync": "svelte-kit sync",
    "check:type": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npm-run-all check:sync check:watch:type",
    "check:watch:type": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --concurrency=auto .",
    "lint:prettier": "prettier --check .",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "type": "module",
  "version": "0.0.1"
}
